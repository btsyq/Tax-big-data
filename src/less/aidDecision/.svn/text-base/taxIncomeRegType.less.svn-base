.box1{
    width: 100%;
    position: relative;
    //表2
    #chart1{
        height: 460px;
        width:49%; 
        float:left; 
        margin-bottom:80px;
    } 
    //表2
    .chartCon{                                     
        position: relative;
        width:49%; 
        float:right; 
        margin-bottom:20px;
        .title{
            font-size:16px;
            text-align: center;
            position: absolute;
            right: 25%;
            top:10%;
            .subtitle{
                color:#fe930d;
                font-weight: bold;
            }
        }
        #chart2{
            height: 460px;
            width: 100%;
        }
        .unit{
            position: absolute;
            right: 2%;
            bottom:2%; 
        }
    }
    //中间线
    .centerLine{                                      
        height:350px; 
        width:2%;
        float:left; 
        margin:55px 0;
        .line{
            width:1px;
            background:#dcdcdc;
            height:100%;
            margin:0 auto;
        }
    }
    //时间轴
    #date-slider{
        width: 80%; 
        border-radius:20px;
        position: absolute;
        bottom:0;
        left:50%;
        margin-left:-40%;
        margin-bottom:50px;
    }
}
