.box1{
    height: 665px;
    width: 100%;
    overflow-x: auto;
    .inputs{
        height: 42px;
        line-height: 42px;
        
        span{
            display: inline-block;
            float: left;
            width: 80px;
        }
        .select_box{
            margin-top: 6px;
            .sel_show{
                width: 200px;
            }
        }
        input[type="text"]{
            border: 1px solid #D2D2D2;
            border-radius: 6px;
            width: 228px;
            height: 28px;
            display: block;
            float: left;
            margin-top: 6px;
            margin-left: 45px;
            outline: none;
            text-indent: 10px;
        }
        button{
            float: left;
            margin-top: 6px;
            margin-left: 69px;
        }
    }
    
    .mytable {
        margin-top: 35px;
        .mycheck{
            top: 5px;
        }
    }
}


#upload{
    width: 600px;
    height: 210px;
    padding: 20px 44px;
    box-sizing: border-box;
    background-color: #fff;
    display: none;
    .cont{
        width: 100%;
        height: 100%;
        .tit{
            height: 40px;
            line-height: 40px;
            i{
                color: #0074f5;
                font-size: 20px;
                font-weight: bold;
                margin-right: 10px;
            }
        }
        p{
            margin-top: 40px;
            margin-bottom: 10px;
        }
        .form{
            line-height: 50px;
            position: relative;
            input[type="text"]{
                border: 1px solid #D2D2D2;
                border-radius: 6px;
                width: 312px;
                height: 28px;
                outline: none;
                text-indent: 10px;
            }
            input[type="button"],input[type="submit"]{
                height: 28px;
                width: 78px;
                border: 1px solid #007cef;
                border-radius: 6px;
                color: #007cef;
                background: #fff;
                cursor: pointer;
                font-family: "微软雅黑";
                margin-left: 15px;
                outline: none;
            }
            input[type="file"]{
                position: absolute;
                display: block;
                top: 12px;
                left: 0;
                height: 29px;
                opacity: 0;
                width: 412px;
                background: red;
            }
        }
    }
}